@model IEnumerable<WalletTransactions>
<table class="table">
    <thead>
        <tr>
            <th>Amount</th>
            <th>Balance</th>
            <th>Factor</th>
            <th>Description</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model?.Count() > 0)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Amount</td>
                    <td>@item.Balance</td>
                    <td>@item.Factor</td>
                    <td>@item.Description </td>
                    <td>
                        <a class="btn btn-secondary btn-rounded btn-fw editSubAdmin" asp-controller="Admin" asp-action="#" >Edit</a>
                    </td>
                </tr>

            }
        }
    </tbody>
</table>